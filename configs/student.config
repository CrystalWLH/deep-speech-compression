[GENERAL]
model_type = student
save_model = ./models/st_w2l_v1
input_channels = 120
data_format = channels_last
conv_type = conv


[FILES]
vocab_path = ./data/tfrecords_data/vocab.pkl
train_data = ./data/tfrecords_data/tfrecords_mfccs.train
eval_data = ./data/tfrecords_data/tfrecords_mfccs.dev_clean
test_data = ./data/tfrecords_data/tfrecords_mfccs.test_clean
teacher_logits = ./data/tfrecords_data/w2l_v1.logits


[TRAIN]
batch_size = 5
epochs = 5
activation = relu
bn = false
temperature = 5
alpha = 0.3
clipping = 0
adam_lr = 1e-4
adam_eps = 1e-8


[STUDENT]
filters = [250]
widths = [ 1]
strides = [ 1]
dropouts = [0]


